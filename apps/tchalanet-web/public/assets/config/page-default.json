    "component": "PublicHeader",
    "properties": {
      "brand": {
        "logo": "/assets/branding/logo-tchalanet.svg",
        "name": "TCHALANET",
        "href": "/"
      },
      "navigation": [
        {
          "labelKey": "nav.home",
          "path": "/"
